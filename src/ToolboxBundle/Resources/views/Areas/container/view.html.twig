{% import '@Toolbox/Macro/wrap.html.twig' as wrap_macro %}
{{ elementConfigBar|raw }}
<div class="toolbox-element toolbox-container {{ pimcore_select('addlCls').getData() }}">
    {{ wrap_macro.createWrapHead(elementThemeConfig) }}
    {% include '@Toolbox/Toolbox/' ~ elementThemeConfig.layout ~ '/Container/view.html.twig' with { 'containerClass' : pimcore_checkbox('fullWidthContainer').isChecked() ? 'container-fluid' : 'container' } %}
    {{ wrap_macro.createWrapFoot(elementThemeConfig) }}
</div>